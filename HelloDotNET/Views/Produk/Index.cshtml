@model IEnumerable<HelloDotNET.Produk>

@{
    ViewBag.Title = "Index";
}

@foreach (var item in Model) {
    <tr>
        
        <td>
            @Html.DisplayFor(modelItem => item.Nama_produk)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Spek)
        </td>
        <td>
            @Html.ActionLink("Details", "Details", new { id=item.Id_Produk }) 
            
        </td>
    </tr>
}

</table>
